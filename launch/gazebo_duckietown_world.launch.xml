<?xml version="1.0"?>
<launch>
  <env name="GAZEBO_RESOURCE_PATH" value="$(find-pkg-share  mirte_gazebo)"/>
  <arg name="gui" default="1"/>
  <arg name="lockstep" default="False"/>
  <arg name="gazebo_extra" value="$(eval '--lockstep' if arg('lockstep') else '')"/>
  <arg name="duckietown" default="$(find-pkg-share  mirte_gazebo)/maps/udem1.yaml"/>
  <param name="/duckietown_generator" command="$(find-pkg-share  mirte_gazebo)/scripts/buildMap.sh $(var duckietown) $(find-pkg-share  mirte_gazebo)/urdf/generated.world $(find-pkg-share  mirte_gazebo)/worlds/generated.world"/>
  <include file="$(find-pkg-share  gazebo_ros)/launch/empty_world.launch.xml">
    <arg name="gui" value="$(var gui)"/>
    <arg name="world_name" value="$(find-pkg-share  mirte_gazebo)/worlds/generated.world"/>
    <arg name="extra_gazebo_args" value="$(var gazebo_extra)"/>
  </include>
</launch>
